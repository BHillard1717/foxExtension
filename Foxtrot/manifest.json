{
  "name": "Foxtrot",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "permissions": [ "tabs", "webNavigation" ],
  "web_accessible_resources": [{
    "matches": ["*://*/*"],
    "resources": ["Fox_Sprite_Sheet.png"]
}]
}
